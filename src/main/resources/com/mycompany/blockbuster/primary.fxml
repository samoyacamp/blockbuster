<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.blockbuster.PrimaryController">
   <children>
      <ListView fx:id="listado" prefHeight="200.0" prefWidth="200.0" />
